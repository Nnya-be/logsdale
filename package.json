{
  "name": "webapp",
  "version": "1.0.0",
  "description": "testing skills",
  "main": "project.js",
  "scripts": {
    "watch:sass": "node-sass sass/project.scss styles/project.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/project.scss project-comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' styles/project-comp.css -o styles/project-prefix.css",
    "compress:css": "node-sass styles/preject-prefix.css styles/project.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Solomon Chamamme",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.12",
    "compress": "^0.99.0",
    "concat": "^1.0.3",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  },
  "dependencies": {
    "all": "^0.0.0",
    "babel": "^6.23.0",
    "prettier": "^2.7.1",
    "run": "^1.4.0"
  }
}
